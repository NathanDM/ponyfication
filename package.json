{
  "name": "ponyfication",
  "version": "0.0.1",
  "description": "Simple notification center for Pony information",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watchify": "watchify --debug app/index.js -o build/bundle.js -v -d",
